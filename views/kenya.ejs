<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<title>Gowth</title>
<script src="/lib/d3.v2.js"></script>
<script src="/lib/jquery-1.8.3.js"></script>
	<script src="/lib/_.js"></script>
	<style>
		
		body {
		  font: 10px sans-serif;
		}

		svg {
			border: solid 1px black;
		}

		

		.area.above {
		  fill: rgb(252,141,89);
		}

		.area.below {
		  fill: rgb(145,207,96);
		}



		.axis path,
		.axis line {
		  fill: none;
		  stroke: #000;
		  shape-rendering: crispEdges;
		}

		 .subMethods.wigglish .axis path {
			stroke:#fff;
		 }

		.y.axis line,
		.x.axis line {
			stroke: rgba(87, 87, 87, 0.1);
			shape-rendering: crispEdges;
		}
		

		.axis.conversion path {
			stroke: purple;
		}


		.y.axis.subs {
			fill: green;
		}


		.line {
		  fill: none;
		  stroke-width: 1.5px;
		}

		.line.conv{ 
			stroke: green;
		}

		.line.visits{
			stroke:darkred;
		}
		

	
	</style>
</head>
<body>
	<h1>Kenya JApp</h1>
	
	<script src="/growth/dashboard.js"></script>
	<script src="/kenya/kenya.js"></script>
	<script>
        
	    ['/kenya/kenya-refs.csv'].forEach(function (path) {
	        var loader = new Dashboard.Growth.DataLoader(path, "%m/%d/%Y");
	        var smoother = new Dashboard.Growth.MovingAverageDataSmoother(7);
	        new Dashboard.Growth.KenyaGraph(loader, smoother);
	    });

	</script>
</body>
</html>
