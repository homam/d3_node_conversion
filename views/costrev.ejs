<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<title></title>
	<script src="/lib/d3.v2.js"></script>
	<script src="/lib/_.js"></script>
	<style>
		
		body {
		  font: 10px sans-serif;
		}

		.axis path,
		.axis line {
		  fill: none;
		  stroke: #000;
		  shape-rendering: crispEdges;
		}

		.x.axis path {
		  display: none;
		}

		text.country {
			font-size:22px;
		}

		.area.above {
		  fill: rgb(145,207,96);
		}

		.area.below {
		  fill: rgb(252,141,89);
		}

		.line {
		  fill: none;
		  stroke: #000;
		  stroke-width: 1.5px;
		}

		.line.rev {
			stroke: darkgreen;
		}

		.line.cost {
			stroke: darkred;
		}

	</style>
</head>
<body>

	<section id="graphs"></section>
	<script src="/costrev/area.js"></script>
</body>
</html>
